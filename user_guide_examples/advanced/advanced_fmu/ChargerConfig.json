{
  "core_name": "Charger",
  "core_type": "zmq",
  "log_level": "warning",
  "name": "Charger",
  "period": 60,
  "terminate_on_error": true,
  "uninterruptible": false,
  "wait_for_current_time_update": true,
  "publications": [
    {
      "global": true,
      "key": "Charger/EV1_voltage",
      "type": "double",
      "unit": "V",
      "targets": [
        "Battery1.Vin"
      ]
    },
    {
      "global": true,
      "key": "Charger/EV2_voltage",
      "type": "double",
      "unit": "V",
      "targets": [
        "Battery2.Vin"
      ]
    },
    {
      "global": true,
      "key": "Charger/EV3_voltage",
      "type": "double",
      "unit": "V",
      "targets": [
        "Battery3.Vin"
      ]
    },
    {
      "global": true,
      "key": "Charger/EV4_voltage",
      "type": "double",
      "unit": "V",
      "targets": [
        "Battery4.Vin"
      ]
    },
    {
      "global": true,
      "key": "Charger/EV5_voltage",
      "type": "double",
      "unit": "V",
